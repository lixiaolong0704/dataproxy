<div class="popup2">
    <div class="popup-header">
        <label>编辑标签</label>
        <div class="popup-close"></div>
    </div>
    <div class='popup-content'>
        <form class="form-horizontal" id="form">
            <input  type="hidden" value="<%= id %>" name ="id">
            <div class="form-group">
                <label for="inputEmail3" class="col-sm-2 control-label">标签名称</label>
                <div class="col-sm-10">
                    <input type="text" class="form-control"  name="name"  value="<%= name %>"   placeholder="Email">
                </div>
            </div>


            <div class="form-group">
                <div class="col-sm-offset-2 col-sm-10">
                    <a type="submit" href="javascript:void(0)" class="btn btn-default save" >保存</a>
                </div>
            </div>
        </form>
    </div>
</div>